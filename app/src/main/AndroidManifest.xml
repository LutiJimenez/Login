<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aaluti.login">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Log_In"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".SobreMi"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Microsoft"
            android:screenOrientation="portrait" />
        <activity android:name=".VisualStudio" />
        <activity android:name=".SqlServer" />
        <activity android:name=".iis" />
        <activity android:name=".Tecnologias" />
        <activity android:name=".Cursos" />
        <activity android:name=".VisorImagenes" />
        <activity android:name=".Detalle" />
        <activity android:name=".Android" />
    </application>

</manifest>